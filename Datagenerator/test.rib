Format 640 480 1
Projection "perspective" "fov" 67
FrameAspectRatio 1.3
ShadingRate 5


ReadArchive "trans.rib"


WorldBegin
    ReverseOrientation
    LightSource "distantlight" 1 "intensity" 1.0
            "from" [0 0 10]
            "to" [0 0 0]
    LightSource "distantlight" 1 "intensity" 1.0
            "from" [0 7 0]
            "to" [0 0 0]
  
    LightSource "distantlight" 1 "intensity" 1.0
            "from" [8 0 0]
            "to" [0 0 0]
			
			
			  LightSource "distantlight" 1 "intensity" 1.0
            "from" [0 0 -10]
            "to" [0 0 0]
    LightSource "distantlight" 1 "intensity" 1.0
            "from" [0 -7 0]
            "to" [0 0 0]
  
    LightSource "distantlight" 1 "intensity" 1.0
            "from" [-8 0 0]
            "to" [0 0 0]
  
    TransformBegin
	    Surface "paintedplastic" "Ks" 0.0 "texturename" "our.tx" 
		
        PointsPolygons  [4 4 4 4 4 4] # six faces each with four vertices
                        [0 1 2 3   4 7 6 5   3 2 6 7   0 4 5 1   2 1 5 6   0 3 7 4]
                "P"     [-75 75 -75   75 75 -75   75 75 75   -75 75 75
                        -75 -75 -75   75 -75 -75   75 -75 75   -75 -75 75]
                "st"    [0.25 0.3   0.5 0.3   0.75 0.3   0.1 0.3   0.25 0.6    0.5 0.6   0.75 0.6   0.1 0.6]
    TransformEnd
WorldEnd
